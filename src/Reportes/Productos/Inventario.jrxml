	<queryString>
		<![CDATA[SELECT
    
     producto.`idproducto` AS producto_idproducto,
     producto.`descripcion_prod` AS producto_descripcion_prod,
     producto.`cantidad_prod` AS producto_cantidad_prod,
     producto.`costo_prod` AS producto_costo_prod,
